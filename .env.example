# 애플리케이션 환경 설정
NODE_ENV=development
PORT=3000
API_PREFIX=api

# 로깅 설정
LOG_LEVEL=info
LOG_FILE_ENABLED=false
LOG_FILE_PATH=./logs

# CORS 설정
CORS_ENABLED=true
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# Rate Limiting 설정
THROTTLE_TTL=60000
THROTTLE_LIMIT=10

# 데이터베이스 설정
DATABASE_TYPE=mysql
DATABASE_HOST=localhost
DATABASE_PORT=3306
DATABASE_NAME=nestjs_db
DATABASE_USER=root
DATABASE_PASSWORD=
DATABASE_SYNCHRONIZE=true
DATABASE_LOGGING=false
